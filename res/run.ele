&run_setup lattice=lattice.lte use_beamline=beamline p_central_mev=0.25
	sigma=%s.sig centroid=%s.cen
&end

&run_control &end

&bunched_beam			!basiert auf Dijkstal
	n_particles_per_bunch=1000
	emit_x=0.45e-6
	beta_x=.24
	alpha_x=-2.13
	emit_y=0.51e-6
	beta_y=2.1
	alpha_y=5.9
	distribution_type[0]="gaussian","gaussian"
	enforce_rms_values[0]=1,1,1
&end

&track &end
